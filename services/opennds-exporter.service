#!/bin/sh /etc/rc.common

START=95
USE_PROCD=1

start_service() {
    procd_open_instance
    procd_set_param command python3 /usr/local/monitoring/exporters/opennds-exporter.py
    procd_set_param stdout 1
    procd_set_param stderr 1
    procd_set_param respawn
    procd_close_instance
}

stop_service() {
    service_stop /usr/local/monitoring/exporters/opennds-exporter.py
}
