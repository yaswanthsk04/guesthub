#!/bin/sh /etc/rc.common

START=99
STOP=15
USE_PROCD=1

boot() {
    start "$@"
}

start_service() {
    procd_open_instance
    procd_set_param command /root/verify-setup.sh
    procd_set_param stdout 1
    procd_set_param stderr 1
    procd_close_instance
}

reload_service() {
    stop
    start
}
